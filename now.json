{
  "version": 2,
  "builds": [
    { "src": "index.py", "use": "@sisp/python-wsgi", "config": { "maxLambdaSize": "15mb" } }
  ],
  "env": {
    "ACCESS_TOKEN": "@access-token",
    "CHANNEL_SECRET": "@channel-secret"
  },
  "routes": [
    { "src": "/.*", "dest": "/"}
  ],
  "regions": ["hnd1"],
  "alias": ["shge-line-bot.now.sh"],
  "github": { "silent": true }
}
